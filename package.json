{
  "name": "ietnitk",
  "description": "Official Blog of IET-NITK",
  "version": "0.1.0",
  "author": "IET NITK Web Team <iet@nitk.edu.in>",
  "bugs": {
    "url": "https://github.com/IET-NITK/iet-nitk.github.io/issues"
  },
  "engines": {
    "npm": ">= 7.0.0",
    "yarn": "please-use-npm",
    "node": ">= 12.5.0"
  },
  "dependencies": {
    "@splidejs/react-splide": "0.7.5",
    "assert": "^2.0.0",
    "bootstrap": "4.6.1",
    "font-awesome": "4.7.0",
    "gatsby": "4.11.3",
    "gatsby-plugin-disqus": "1.2.3",
    "gatsby-plugin-google-analytics": "4.11.0",
    "gatsby-plugin-image": "^2.8.1",
    "gatsby-plugin-manifest": "4.11.1",
    "gatsby-plugin-react-helmet": "5.11.0",
    "gatsby-plugin-sass": "5.11.1",
    "gatsby-plugin-sharp": "4.11.1",
    "gatsby-remark-copy-linked-files": "5.11.0",
    "gatsby-remark-images": "6.11.1",
    "gatsby-remark-prismjs": "6.11.0",
    "gatsby-remark-responsive-iframe": "5.11.0",
    "gatsby-remark-smartypants": "5.11.0",
    "gatsby-source-filesystem": "4.11.1",
    "gatsby-source-rss-feed": "^1.2.2",
    "gatsby-source-strapi": "1.0.3",
    "gatsby-transformer-remark": "5.11.1",
    "gatsby-transformer-sharp": "4.11.0",
    "gatsby-transformer-yaml": "4.11.0",
    "gh-pages": "3.2.3",
    "graphql": "15.8.0",
    "prismjs": "1.27.0",
    "react": "17.0.2",
    "react-bootstrap": "2.2.3",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-markdown": "8.0.2",
    "react-share": "4.4.0",
    "react-ticker": "1.3.1",
    "remark-gfm": "^3.0.1",
    "tripetto-runner-autoscroll": "^5.4.1",
    "tripetto-runner-foundation": "^6.2.5",
    "tripetto-services": "^2.0.0",
    "typeface-merriweather": "1.1.13",
    "typeface-montserrat": "1.1.13"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-preset-gatsby": "2.11.1",
    "eslint": "7.32.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "node-sass": "5.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.2"
  },
  "homepage": "https://iet.nitk.ac.in",
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "*.{js,css,html}": [
      "prettier  --print-width 100 --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "license": "0BSD",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/IET-NITK/iet-nitk.github.io.git"
  },
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true  gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "lint": "npx eslint . --fix"
  },
  "renovate": {
    "timezone": "Asia/Kolkata",
    "extends": [
      "config:base"
    ],
    "schedule": [
      "every weekend"
    ],
    "packageRules": [
      {
        "matchDepTypes": [
          "devDependencies"
        ],
        "addLabels": [
          "dev"
        ],
        "prPriority": 5
      },
      {
        "matchDepTypes": [
          "optionalDependencies"
        ],
        "addLabels": [
          "optional"
        ],
        "prPriority": 4
      }
    ]
  }
}
